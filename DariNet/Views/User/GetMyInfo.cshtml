
@{
    ViewBag.Title = "GetMyInfo";
}

<h2>GetMyInfo</h2>
<a href="@Url.Action("Create","User")">add new user</a>
<div>@Html.ActionLink("welcome", "Index")</div>

<table cellpadding="2" cellspacing="2" border="1">
    <tr>
        <th>userId</th>
        <th>name</th>
        <th>email</th>
        <th>gender</th>
        <th>address</th>
        <th>dateCreate</th>
        <th>lastLoginDate</th>
        <th>point</th>
    </tr>
    @foreach (var p in ViewBag.result)
    {
        <tr>
            <td>@p.userId</td>
            <td>@p.name</td>
            <td>@p.email</td>
            <td>@p.sex</td>
            <td>@p.address</td>
            <td>@p.dateCreate</td>
            <td>@p.lastLoginDate</td>
            <td>@p.point</td>
        </tr>
    }
</table>
